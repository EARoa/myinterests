<!DOCTYPE html>
<html>
<head>
  <title>myInterest</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>



<div class="container">

<table align="right">
  <tr>
    <td>
      <%= link_to "New", "#", class: "btn btn-success", data: {toggle: "modal", target: "#newLinkModal"} %>
    </td>
    <td>
      <% if user_signed_in? %>
        Welcome back <%= current_user.email %>!
        <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
      <% end %>
    </td>

  </tr>
</table>



  <p class="notice"><%= notice %></p>

  <p class="alert"><%= alert %></p>



<div id="modaldiv"> <!-- Modal Div -->
  <!-- Modal -->
<div class="modal fade" id="newLinkModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">

      <div>

        <%= render partial: 'links/form' %>

      </div>

    </div>
  </div>
</div>

</div> <!-- Modal Div -->
  <%= yield %>





</div>  <!-- Container Div -->

</body>
</html>
